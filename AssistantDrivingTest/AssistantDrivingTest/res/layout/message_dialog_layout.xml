<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/frame_container"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical" >    <TextView        android:id="@+id/voice"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:gravity="center"        android:padding="20dp"        android:textColor="@drawable/dialog_text_color"        android:text="@string/add_item"        android:textSize="20sp" />    <ImageView        android:layout_width="match_parent"        android:layout_height="1dp"        android:src="#cccccc" />    <TextView        android:id="@+id/start"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:gravity="center"        android:padding="20dp"        android:enabled="false"        android:textColor="@drawable/dialog_text_color"        android:text="@string/start"        android:textSize="20sp" />    <ImageView        android:id="@+id/divide"        android:layout_width="match_parent"        android:layout_height="1dp"        android:src="#cccccc" />    <TextView        android:id="@+id/end"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:gravity="center"        android:padding="20dp"        android:enabled="false"        android:textColor="@drawable/dialog_text_color"        android:text="@string/setting_end"        android:textSize="20sp" /></LinearLayout>